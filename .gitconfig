[alias]
  a  = add
  b  = branch
  br = branch
  d  = diff
  ds = diff --staged
  ci = commit
  co = checkout
  cp = cherry-pick
  l  = log
  la = log --graph --all
  lg = log --graph
  ln = log --name-status
  s  = status
  st = status
[color]
  branch = auto
  diff   = auto
  status = auto
[format]
  pretty = format:%Cblue%cd %Cgreen%h %Creset%s %Cblue(%cN)%Cgreen%d
[merge]
  tool = vimdiff
[log]
  date = format:%F %I:%M %p
[pull]
  rebase = true
[push]
  default = upstream
